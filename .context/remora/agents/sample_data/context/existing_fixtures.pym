from grail import Input

existing_fixtures: str = Input("existing_fixtures", default="")


fixtures_list: list[dict[str, str]] = []

if existing_fixtures:
    try:
        lines = existing_fixtures.splitlines()
        for line in lines:
            line = line.strip()
            if line.startswith("{"):
                name = ""
                path = ""
                if '"name"' in line:
                    start = line.find('"name"') + 6
                    end = line.find('}', start)
                    if start > 5 and end > start:
                        name_val = line[start:end].strip()
                        if name_val.startswith('"') and name_val.endswith('"'):
                            name = name_val[1:-1]
                if '"path"' in line:
                    start = line.find('"path"') + 6
                    end = line.find('}', start)
                    if start > 5 and end > start:
                        path_val = line[start:end].strip()
                        if path_val.startswith('"') and path_val.endswith('"'):
                            path = path_val[1:-1]
                if name or path:
                    fixtures_list.append({"name": name, "path": path})
    except Exception:
        pass

result = fixtures_list
result
